(e,t){return e.type===t}function vE(e){const t=[],n=bO(e);let r="BOD",o=0,i=n[o];for(;!Ag(i,DI);){let s=0;const a=[];if(r==="BOD")if(i.text==="M"||i.text==="m")o++,s=Qh[i.text],r=i.text;else return vE("M0,0"+e);else Ag(i,F2)?s=Qh[r]:(o++,s=Qh[i.text],r=i.text);if(o+s<n.length){for(let l=o;l<o+s;l++){const c=n[l];if(Ag(c,F2))a[a.length]=+c.text;else throw new Error("Param not a number: "+r+","+c.text)}if(typeof Qh[r]=="number"){const l={key:r,data:a};t.push(l),o+=s,i=n[o],r==="M"&&(r="L"),r==="m"&&(r="l")}else throw new Error("Bad segment: "+r)}else throw new Error("Path data ended short")}return t}function OI(e){let t=0,n=0,r=0,o=0;const i=[];for(const{key:s,data:a}of e)switch(s){case"M":i.push({key:"M",data:[...a]}),[t,n]=a,[r,o]=a;break;case"m":t+=a[0],n+=a[1],i.push({key:"M",data:[t,n]}),r=t,o=n;break;case"L":i.push({key:"L",data:[...a]}),[t,n]=a;break;case"l":t+=a[0],n+=a[1],i.push({key:"L",data:[t,n]});break;case"C":i.push({key:"C",data:[...a]}),t=a[4],n=a[5];break;case"c":{const l=a.map((c,d)=>d%2?c+n:c+t);i.push({key:"C",data:l}),t=l[4],n=l[5];break}case"Q":i.push({key:"Q",data:[...a]}),t=a[2],n=a[3];break;case"q":{const l=a.map((c,d)=>d%2?c+n:c+t);i.push({key:"Q",data:l}),t=l[2],n=l[3];break}case"A":i.push({key:"A",data:[...a]}),t=a[5],n=a[6];break;case"a":t+=a[5],n+=a[6],i.push({key:"A",data:[a[0],a[1],a[2],a[3],a[4],t,n]});break;case"H":i.push({key:"H",data:[...a]}),t=a[0];break;case"h":t+=a[0],i.push({key:"H",data:[t]});break;case"V":i.push({key:"V",data:[...a]}),n=a[0];break;case"v":n+=a[0],i.push({key:"V",data:[n]});break;case"S":i.push({key:"S",data:[...a]}),t=a[2],n=a[3];break;case"s":{const l=a.map((c,d)=>d%2?c+n:c+t);i.push({key:"S",data:l}),t=l[2],n=l[3];break}case"T":i.push({key:"T",data:[...a]}),t=a[0],n=a[1];break;case"t":t+=a[0],n+=a[1],i.push({key:"T",data:[t,n]});break;case"Z":case"z":i.push({key:"Z",data:[]}),t=r,n=o;break}return i}function NI(e){const t=[];let n="",r=0,o=0,i=0,s=0,a=0,l=0;for(const{key:c,data:d}of e){switch(c){case"M":t.push({key:"M",data:[...d]}),[r,o]=d,[i,s]=d;break;case"C":t.push({key:"C",data:[...d]}),r=d[4],o=d[5],a=d[2],l=d[3];break;case"L":t.push({key:"L",data:[...d]}),[r,o]=d;break;case"H":r=d[0],t.push({key:"L",data:[r,o]});break;case"V":o=d[0],t.push({key:"L",data:[r,o]});break;case"S":{let u=0,f=0;n==="C"||n==="S"?(u=r+(r-a),f=o+(o-l)):(u=r,f=o),t.push({key:"C",data:[u,f,...d]}),a=d[0],l=d[1],r=d[2],o=d[3];break}case"T":{const[u,f]=d;let p=0,m=0;n==="Q"||n==="T"?(p=r+(r-a),m=o+(o-l)):(p=r,m=o);const g=r+2*(p-r)/3,y=o+2*(m-o)/3,v=u+2*(p-u)/3,E=f+2*(m-f)/3;t.push({key:"C",data:[g,y,v,E,u,f]}),a=p,l=m,r=u,o=f;break}case"Q":{const[u,f,p,m]=d,g=r+2*(u-r)/3,y=o+2*(f-o)/3,v=p+2*(u-p)/3,E=m+2*(f-m)/3;t.push({key:"C",data:[g,y,v,E,p,m]}),a=u,l=f,r=p,o=m;break}case"A":{const u=Math.abs(d[0]),f=Math.abs(d[1]),p=d[2],m=d[3],g=d[4],y=d[5],v=d[6];u===0||f===0?(t.push({key:"C",data:[r,o,y,v,y,v]}),r=y,o=v):(r!==y||o!==v)&&(FI(r,o,y,v,u,f,p,m,g).forEach(function(w){t.push({key:"C",data:w})}),r=y,o=v);break}case"Z":t.push({key:"Z",data:[]}),r=i,o=s;break}n=c}return t}function _O(e){return Math.PI*e/180}function Xc(e,t,n){const r=e*Math.cos(n)-t*Math.sin(n),o=e*Math.sin(n)+t*Math.cos(n);return[r,o]}function FI(e,t,n,r,o,i,s,a,l,c){const d=_O(s);let u=[],f=0,p=0,m=0,g=0;if(c)[f,p,m,g]=c;else{[e,t]=Xc(e,t,-d),[n,r]=Xc(n,r,-d);const L=(e-n)/2,R=(t-r)/2;let $=L*L/(o*o)+R*R/(i*i);$>1&&($=Math.sqrt($),o=$*o,i=$*i);const G=a===l?-1:1,K=o*o,Z=i*i,B=K*Z-K*R*R-Z*L*L,U=K*R*R+Z*L*L,N=G*Math.sqrt(Math.abs(B/U));m=N*o*R/i+(e+n)/2,g=N*-i*L/o+(t+r)/2,f=Math.asin(parseFloat(((t-g)/i).toFixed(9))),p=Math.asin(parseFloat(((r-g)/i).toFixed(9))),e<m&&(f=Math.PI-f),n<m&&(p=Math.PI-p),f<0&&(f=Math.PI*2+f),p<0&&(p=Math.PI*2+p),l&&f>p&&(f=f-Math.PI*2),!l&&p>f&&(p=p-Math.PI*2)}let y=p-f;if(Math.abs(y)>Math.PI*120/180){const L=p,R=n,$=r;l&&p>f?p=f+Math.PI*120/180*1:p=f+Math.PI*120/180*-1,n=m+o*Math.cos(p),r=g+i*Math.sin(p),u=FI(n,r,R,$,o,i,s,0,l,[p,L,m,g])}y=p-f;const v=Math.cos(f),E=Math.sin(f),w=Math.cos(p),S=Math.sin(p),T=Math.tan(y/4),_=4/3*o*T,k=4/3*i*T,C=[e,t],P=[e+_*E,t-k*v],M=[n+_*S,r-k*w],O=[n,r];if(P[0]=2*C[0]-P[0],P[1]=2*C[1]-P[1],c)return[P,M,O].concat(u);{u=[P,M,O].concat(u);const L=[];for(let R=0;R<u.length;R+=3){const $=Xc(u[R][0],u[R][1],d),G=Xc(u[R+1][0],u[R+1][1],d),K=Xc(u[R+2][0],u[R+2][1],d);L.push([$[0],$[1],G[0],G[1],K[0],K[1]])}return L}}const xO={randOffset:TO,randOffsetWithRange:AO,ellipse:IO,doubleLineOps:MO};function $I(e,t,n,r,o){return{type:"path",ops:ai(e,t,n,r,o)}}function zf(e,t,n){const r=(e||[]).length;if(r>2){const o=[];for(let i=0;i<r-1;i++)o.push(...ai(e[i][0],e[i][1],e[i+1][0],e[i+1][1],n));return t&&o.push(...ai(e[r-1][0],e[r-1][1],e[0][0],e[0][1],n)),{type:"path",ops:o}}else if(r===2)return $I(e[0][0],e[0][1],e[1][0],e[1][1],n);return{type:"path",ops:[]}}function SO(e,t){return zf(e,!0,t)}function kO(e,t,n,r,o){const i=[[e,t],[e+n,t],[e+n,t+r],[e,t+r]];return SO(i,o)}function U3(e,t){let n=G3(e,1*(1+t.roughness*.2),t);if(!t.disableMultiStroke){const r=G3(e,1.5*(1+t.roughness*.22),PO(t));n=n.concat(r)}return{type:"path",ops:n}}function IO(e,t,n,r,o){const i=zI(n,r,o);return $2(e,t,o,i).opset}function zI(e,t,n){const r=Math.sqrt(Math.PI*2*Math.sqrt((Math.pow(e/2,2)+Math.pow(t/2,2))/2)),o=Math.ceil(Math.max(n.curveStepCount,n.curveStepCount/Math.sqrt(200)*r)),i=Math.PI*2/o;let s=Math.abs(e/2),a=Math.abs(t/2);const l=1-n.curveFitting;return s+=ve(s*l,n),a+=ve(a*l,n),{increment:i,rx:s,ry:a}}function $2(e,t,n,r){const[o,i]=j3(r.increment,e,t,r.rx,r.ry,1,r.increment*Np(.1,Np(.4,1,n),n),n);let s=Fp(o,null,n);if(!n.disableMultiStroke&&n.roughness!==0){const[a]=j3(r.increment,e,t,r.rx,r.ry,1.5,0,n),l=Fp(a,null,n);s=s.concat(l)}return{estimatedPoints:i,opset:{type:"path",ops:s}}}function H3(e,t,n,r,o,i,s,a,l){const c=e,d=t;let u=Math.abs(n/2),f=Math.abs(r/2);u+=ve(u*.01,l),f+=ve(f*.01,l);let p=o,m=i;for(;p<0;)p+=Math.PI*2,m+=Math.PI*2;m-p>Math.PI*2&&(p=0,m=Math.PI*2);const g=Math.PI*2/l.curveStepCount,y=Math.min(g/2,(m-p)/2),v=Y3(y,c,d,u,f,p,m,1,l);if(!l.disableMultiStroke){const E=Y3(y,c,d,u,f,p,m,1.5,l);v.push(...E)}return s&&(a?v.push(...ai(c,d,c+u*Math.cos(p),d+f*Math.sin(p),l),...ai(c,d,c+u*Math.cos(m),d+f*Math.sin(m),l)):v.push({op:"lineTo",data:[c,d]},{op:"lineTo",data:[c+u*Math.cos(p),d+f*Math.sin(p)]})),{type:"path",ops:v}}function W3(e,t){const n=NI(OI(vE(e))),r=[];let o=[0,0],i=[0,0];for(const{key:s,data:a}of n)switch(s){case"M":{i=[a[0],a[1]],o=[a[0],a[1]];break}case"L":r.push(...ai(i[0],i[1],a[0],a[1],t)),i=[a[0],a[1]];break;case"C":{const[l,c,d,u,f,p]=a;r.push(...LO(l,c,d,u,f,p,i,t)),i=[f,p];break}case"Z":r.push(...ai(i[0],i[1],o[0],o[1],t)),i=[o[0],o[1]];break}return{type:"path",ops:r}}function Mg(e,t){const n=[];for(const r of e)if(r.length){const o=t.maxRandomnessOffset||0,i=r.length;if(i>2){n.push({op:"move",data:[r[0][0]+ve(o,t),r[0][1]+ve(o,t)]});for(let s=1;s<i;s++)n.push({op:"lineTo",data:[r[s][0]+ve(o,t),r[s][1]+ve(o,t)]})}}return{type:"fillPath",ops:n}}function hl(e,t){return yO(t,xO).fillPolygons(e,t)}function CO(e,t,n,r,o,i,s){const a=e,l=t;let c=Math.abs(n/2),d=Math.abs(r/2);c+=ve(c*.01,s),d+=ve(d*.01,s);let u=o,f=i;for(;u<0;)u+=Math.PI*2,f+=Math.PI*2;f-u>Math.PI*2&&(u=0,f=Math.PI*2);const p=(f-u)/s.curveStepCount,m=[];for(let g=u;g<=f;g=g+p)m.push([a+c*Math.cos(g),l+d*Math.sin(g)]);return m.push([a+c*Math.cos(f),l+d*Math.sin(f)]),m.push([a,l]),hl([m],s)}function TO(e,t){return ve(e,t)}function AO(e,t,n){return Np(e,t,n)}function MO(e,t,n,r,o){return ai(e,t,n,r,o,!0)}function PO(e){const t=Object.assign({},e);return t.randomizer=void 0,e.seed&&(t.seed=e.seed+1),t}function BI(e){return e.randomizer||(e.randomizer=new RI(e.seed||0)),e.randomizer.next()}function Np(e,t,n,r=1){return n.roughness*r*(BI(n)*(t-e)+e)}function ve(e,t,n=1){return Np(-e,e,t,n)}function ai(e,t,n,r,o,i=!1){const s=i?o.disableMultiStrokeFill:o.disableMultiStroke,a=V3(e,t,n,r,o,!0,!1);if(s)return a;const l=V3(e,t,n,r,o,!0,!0);return a.concat(l)}function V3(e,t,n,r,o,i,s){const a=Math.pow(e-n,2)+Math.pow(t-r,2),l=Math.sqrt(a);let c=1;l<200?c=1:l>500?c=.4:c=-.0016668*l+1.233334;let d=o.maxRandomnessOffset||0;d*d*100>a&&(d=l/10);const u=d/2,f=.2+BI(o)*.2;let p=o.bowing*o.maxRandomnessOffset*(r-t)/200,m=o.bowing*o.maxRandomnessOffset*(e-n)/200;p=ve(p,o,c),m=ve(m,o,c);const g=[],y=()=>ve(u,o,c),v=()=>ve(d,o,c),E=o.preserveVertices;return i&&(s?g.push({op:"move",data:[e+(E?0:y()),t+(E?0:y())]}):g.push({op:"move",data:[e+(E?0:ve(d,o,c)),t+(E?0:ve(d,o,c))]})),s?g.push({op:"bcurveTo",data:[p+e+(n-e)*f+y(),m+t+(r-t)*f+y(),p+e+2*(n-e)*f+y(),m+t+2*(r-t)*f+y(),n+(E?0:y()),r+(E?0:y())]}):g.push({op:"bcurveTo",data:[p+e+(n-e)*f+v(),m+t+(r-t)*f+v(),p+e+2*(n-e)*f+v(),m+t+2*(r-t)*f+v(),n+(E?0:v()),r+(E?0:v())]}),g}function G3(e,t,n){const r=[];r.push([e[0][0]+ve(t,n),e[0][1]+ve(t,n)]),r.push([e[0][0]+ve(t,n),e[0][1]+ve(t,n)]);for(let o=1;o<e.length;o++)r.push([e[o][0]+ve(t,n),e[o][1]+ve(t,n)]),o===e.length-1&&r.push([e[o][0]+ve(t,n),e[o][1]+ve(t,n)]);return Fp(r,null,n)}function Fp(e,t,n){const r=e.length,o=[];if(r>3){const i=[],s=1-n.curveTightness;o.push({op:"move",data:[e[1][0],e[1][1]]});for(let a=1;a+2<r;a++){const l=e[a];i[0]=[l[0],l[1]],i[1]=[l[0]+(s*e[a+1][0]-s*e[a-1][0])/6,l[1]+(s*e[a+1][1]-s*e[a-1][1])/6],i[2]=[e[a+1][0]+(s*e[a][0]-s*e[a+2][0])/6,e[a+1][1]+(s*e[a][1]-s*e[a+2][1])/6],i[3]=[e[a+1][0],e[a+1][1]],o.push({op:"bcurveTo",data:[i[1][0],i[1][1],i[2][0],i[2][1],i[3][0],i[3][1]]})}if(t&&t.length===2){const a=n.maxRandomnessOffset;o.push({op:"lineTo",data:[t[0]+ve(a,n),t[1]+ve(a,n)]})}}else r===3?(o.push({op:"move",data:[e[1][0],e[1][1]]}),o.push({op:"bcurveTo",data:[e[1][0],e[1][1],e[2][0],e[2][1],e[2][0],e[2][1]]})):r===2&&o.push(...ai(e[0][0],e[0][1],e[1][0],e[1][1],n));return o}function j3(e,t,n,r,o,i,s,a){const l=a.roughness===0,c=[],d=[];if(l){e=e/4,d.push([t+r*Math.cos(-e),n+o*Math.sin(-e)]);for(let u=0;u<=Math.PI*2;u=u+e){const f=[t+r*Math.cos(u),n+o*Math.sin(u)];c.push(f),d.push(f)}d.push([t+r*Math.cos(0),n+o*Math.sin(0)]),d.push([t+r*Math.cos(e),n+o*Math.sin(e)])}else{