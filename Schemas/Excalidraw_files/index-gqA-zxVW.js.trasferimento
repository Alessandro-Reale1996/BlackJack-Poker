function __vite__mapDeps(indexes) {
  if (!__vite__mapDeps.viteFileDeps) {
    __vite__mapDeps.viteFileDeps = ["assets/pica-Bx9cj3s0.js","assets/_commonjs-dynamic-modules-h-SxKiO4.js","assets/index.esm-qhsOuAQM.js","assets/index.esm-yEqDn7ZI.js","assets/index.esm-Zx2hheSV.js","assets/index.esm-WlStkW2s.js"]
  }
  return indexes.map((i) => __vite__mapDeps.viteFileDeps[i])
}
